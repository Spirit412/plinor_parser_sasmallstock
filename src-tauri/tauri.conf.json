{
    "build": {
        "devPath": "../src",
        "distDir": "../src",
        "withGlobalTauri": true
    },
    "package": {
        "productName": "plinor_parser_sasmallstock",
        "version": "0.0.1"
    },
    "tauri": {
        "allowlist": {
            "all": false,
            "window": {
                "all": true
            },
            "shell": {
                "all": false,
                "open": true
            }
        },
        "windows": [
            {
                "title": "Плинор: парсер животных",
                "width": 800,
                "height": 600,
                "resizable": false,
                "fullscreen": false
            }
        ],
        "security": {
            "csp": null
        },
        "bundle": {
            "active": true,
            "targets": "all",
            "identifier": "com.plinor.desctop",
            "icon": [
                "icons/32x32.png",
                "icons/128x128.png",
                "icons/128x128@2x.png",
                "icons/icon.icns",
                "icons/icon.ico"
            ],
            "windows": {
                "certificateThumbprint": null,
                "digestAlgorithm": "sha256",
                "timestampUrl": ""
            }
        }
    }
}